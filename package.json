{
  "name": "waiit",
  "version": "4.0.0",
  "description": "Promise-based delay utility. Tiny, typed, zero dependencies.",
  "main": "app.cjs",
  "module": "app.js",
  "browser": "app.global.js",
  "type": "module",
  "types": "app.d.ts",
  "bin": "app.cli.js",
  "exports": {
    ".": {
      "import": {
        "types": "./app.d.ts",
        "default": "./app.js"
      },
      "require": {
        "types": "./app.cjs.d.ts",
        "default": "./app.cjs"
      },
      "script": {
        "types": "./app.global.d.ts",
        "default": "./app.global.js"
      }
    }
  },
  "sideEffects": false,
  "repository": "Raju/waiit",
  "keywords": [
    "wait",
    "delay",
    "sleep",
    "timeout",
    "promise",
    "async",
    "await",
    "setTimeout",
    "timer",
    "pause",
    "browser",
    "node",
    "cli"
  ],
  "author": "Raju Dhami <raju@rajudhami.com>",
  "contributors": [
    "Raju Dhami <raju@rajudhami.com>"
  ],
  "license": "MIT",
  "files": [
    "app.js",
    "app.d.ts",
    "app.cjs",
    "app.cjs.d.ts",
    "app.global.js",
    "app.global.d.ts",
    "app.cli.js"
  ],
  "packageManager": "bun@1.3.8",
  "engines": {
    "node": ">=14"
  }
}